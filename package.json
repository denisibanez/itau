    "jest": "^28.1.3",
    "ng2-currency-mask": "^13.0.3",
    "ngx-cookie-service": "^15.0.0",
    "prop-types": "^15.8.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.7",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
